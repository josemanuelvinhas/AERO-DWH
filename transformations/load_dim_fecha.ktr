      <jsScript>
        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>//Create a Locale according to the specified language code
var locale = new java.util.Locale(
    language_code.getString()
,   country_code.getString()
);

//Create a calendar, use the specified initial date
var calendar = new java.util.GregorianCalendar(locale);
calendar.setTime(initial_date.getDate());

//set the calendar to the current date by adding DaySequence days
calendar.add(calendar.DAY_OF_MONTH,DaySequence.getInteger() - 1);

//get the calendar date
var fecha = new java.util.Date(calendar.getTimeInMillis());
//en-us example: 9/3/07

var dateFormat = java.text.SimpleDateFormat("yyyy-MM-dd");
var fecha_mysql = dateFormat.format(fecha);

var simpleDateFormat   = java.text.SimpleDateFormat("D",locale);
//dia en mes: 1..31
simpleDateFormat.applyPattern("d");
var dia_en_mes      = simpleDateFormat.format(fecha);

//en-us example: "Monday"
simpleDateFormat.applyPattern("EEEE");
var dia_nombre           = simpleDateFormat.format(fecha);

//semana en anho, 1..53
simpleDateFormat.applyPattern("ww");
var semana_en_anho       = simpleDateFormat.format(fecha);

//numero mes en anho, 1..12
simpleDateFormat.applyPattern("MM");
var mes_numero      = simpleDateFormat.format(fecha);

//en-us example: "September"
simpleDateFormat.applyPattern("MMMM");
var mes_nombre         = simpleDateFormat.format(fecha);


//4 digitos representacion del anho, ejemplo:  2007
simpleDateFormat.applyPattern("yyyy");
var anho              = "" + simpleDateFormat.format(fecha);


//manejo de trimestres es un DIY
var trimestre;
switch(parseInt(mes_numero)){
    case 1: case 2: case 3: trimestre = "1"; break;
    case 4: case 5: case 6: trimestre = "2"; break;
    case 7: case 8: case 9: trimestre = "3"; break;
    case 10: case 11: case 12: trimestre = "4"; break;
}

//get the local yes/no values
var si = local_yes.getString();
var no = local_no.getString();

//inicializar para calculos de dias de la semana
var dia_de_semana = java.util.Calendar.DAY_OF_WEEK;

//comprobar si es fin de semana 
var es_fin_de_semana;
if(dia_de_semana==7 || dia_de_semana==1){
	es_fin_de_semana = si;
} else {
	es_fin_de_semana = no;	
}


var clave_fecha = anho + mes_numero + (dia_en_mes&lt;10?"0":"") + dia_en_mes;</jsScript_script>
